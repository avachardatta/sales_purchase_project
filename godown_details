

CREATE TABLE godown_details
(
	gd_godown_code varchar(20) not null,
	gd_godown_name varchar(50) not null,
	gd_godown_add varchar(200) not null,
	gd_godown_city varchar(20) not null,
	gd_godown_pincode numeric not null,
	gd_godown_state varchar(20) not null,
	gd_godown_country varchar(10) not null,
	gd_godown_tel_no numeric not null,
	gd_godown_email varchar(50)
);

SELECT * FROM godown_details;

