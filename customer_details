

CREATE TABLE custoemr_details
(
	cust_add_code NUMERIC NOT NULL,
	cust_code VARCHAR(50) NOT NULL,
	cust_name VARCHAR(100) NOT NULL,
	cust_address VARCHAR(100) NOT NULL,
	cust_city VARCHAR(50) NOT NULL,
	cust_pincode NUMERIC NOT NULL,
	cust_state VARCHAR(50) NOT NULL,
	cust_country VARCHAR(20) NOT NULL,
	cust_mobile_no NUMERIC NOT NULL,
	cust_email VARCHAR(50) NOT NULL ,
	cust_acc_handleby NUMERIC NOT NULL
);

select * from customer_details


