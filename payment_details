

CREATE TABLE payment_details 
(
    pd_no INT,
    pd_payment_date DATE,
    pd_order_no INT,
    pd_order_amt NUMERIC(10,2),
    pd_pay_code INT,
    pd_cust_code VARCHAR(10)
);


SELECT * FROM payment_details;

