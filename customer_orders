CREATE TABLE customer_orders
(
	co_no INT NOT NULL, 
	co_oderdate DATE,
	co_cust_code VARCHAR(20) NOT NULL,
	co_cust_name VARCHAR(100) NOT NULL,
	co_qty INT,
	co_item_price DECIMAL(10,2) NOT NULL,
	co_amt INT,
	co_sd_userid INT
);

